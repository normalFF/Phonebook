<Application x:Class="PhoneBookWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:model="clr-namespace:PhoneBookWPF.Models"
             xmlns:local="clr-namespace:PhoneBookWPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/IconDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <model:ModelLocator x:Key="Locator"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
